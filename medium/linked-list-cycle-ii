// 快慢指针，听说叫Floyd算法，其实就是先碰到，然后以1的速度两边同时走，就能到环开始的地方
// 具体证明搜find-the-duplicate-number  287
/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    ListNode *detectCycle(ListNode *head) {
        if(!head) return NULL;
        ListNode* fast=head,*slow=head;
        do{
            if(!fast->next||!fast->next->next) return NULL;
            fast=fast->next->next;
            slow=slow->next;
        }while(fast!=slow);
        slow=head;
        while(fast!=slow){
            fast=fast->next;
            slow=slow->next;
        }
        return slow;
    }
};
